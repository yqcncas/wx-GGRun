<view class="content" style="{{'background-image:'+('url('+bgBase64+')')+';'}}"><view class="content-box"><input class="content-item" type="number" maxlength="11" placeholder="请输入手机号" placeholder-style="font-family: PingFangSC-Regular;font-size: 14px;color: rgba(9,2,62,0.30);" data-event-opts="{{[['blur',[['checkPhoneNumber',['$event']]]],['input',[['__set_model',['','userPhone','$event',[]]]]]]}}" value="{{userPhone}}" bindblur="__e" bindinput="__e"/><view class="code"><input class="code-item" type="number" placeholder="请输入验证码" placeholder-style="font-family: PingFangSC-Regular;font-size: 14px;color: rgba(9,2,62,0.30);" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" class="{{['getCode',(computedTimer)?'timer':'']}}" bindtap="__e">{{getYzm?'获取验证码':computedTimer+'s'}}</view></view><input class="content-item" type="password" placeholder="请输入密码" placeholder-style="font-family: PingFangSC-Regular;font-size: 14px;color: rgba(9,2,62,0.30);" data-event-opts="{{[['input',[['__set_model',['','userPwd','$event',[]]]]]]}}" value="{{userPwd}}" bindinput="__e"/><input class="content-item" type="password" placeholder="请重复密码" placeholder-style="font-family: PingFangSC-Regular;font-size: 14px;color: rgba(9,2,62,0.30);" data-event-opts="{{[['input',[['__set_model',['','againUserPwd','$event',[]]]]]]}}" value="{{againUserPwd}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['loginButton',['$event']]]]]}}" class="loginButton" bindtap="__e">立即注册</view></view></view>